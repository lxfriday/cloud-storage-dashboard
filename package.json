{
  "name": "cloud-storage-dashboard",
  "displayName": "云存储管理",
  "description": "vscode extexsion for cloud storage",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cloud-storage-dashboard.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "cloud-storage-dashboard.start",
        "title": "打开云存储管理扩展"
      }
    ],
    "viewsContainers": {},
    "views": {}
  },
  "scripts": {
    "ins": "yarn install && cd react-app && yarn install",
    "dev": "cd react-app && npm run dev",
    "vscode:prepublish": "yarn run package",
    "compile": "webpack --mode production",
    "watch": "webpack --watch --mode development",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/async": "^3.2.12",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.65.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "prettier": "^2.6.1",
    "ts-loader": "^9.2.8",
    "typescript": "^4.5.5",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "async": "^3.2.3",
    "axios": "^0.26.1",
    "dayjs": "^1.11.0",
    "nodejs-file-downloader": "^4.9.3",
    "open": "^8.4.0",
    "qiniu": "^7.4.0"
  }
}
